<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="FictionalInvestment.PublicServices.CrmPublicService">
        <endpoint address="net.tcp://localhost:9001/FictionalInvestment/CrmPublicService" 
                  binding="netTcpBinding" 
                  contract="Common.Contracts.ICrmPublicService"/>
      </service>
    </services>

    <client>
      <endpoint address="net.tcp://localhost:9000/FictionalInvestment/CrmDataService" 
                binding="netTcpBinding" 
                contract="Common.Contracts.ICrmDataService"/>
    </client>
  </system.serviceModel>

  <system.diagnostics>
    <trace>
      <listeners>
        <add name="configConsoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
